<html>
  <head>
  </head>
  <body>
    <h2>
      <% path.to_a.each_with_index do |dir, idx| %>
        <% if idx < path.to_a.length - 1 %>
          <a href="./<%= "../"*(path.to_a.length - idx - 1)  %>index.html"><%= dir %></a>
        <% else %>
          <%= dir %>
        <% end %>/
      <% end %>
    </h2>
    <h3><%= data_type %></h3>
    <%= content %>
  </body>
</html>
